<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.enzhi.quadrubbble">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/quadrubbble_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
    <activity android:name=".LoginActivity">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity android:name=".AuthActivity"/>
    <activity android:name=".MainActivity"/>
    <activity android:name=".view.shot_detail.ShotActivity"/>
    <activity android:name=".view.bucket_list.BucketShotListActivity"/>
    <activity android:name=".view.bucket_list.BucketListActivity"/>
    <activity android:name=".view.user.UserActivity"/>
    <activity android:name=".view.user.UserShotListActivity"/>
</application>

</manifest>